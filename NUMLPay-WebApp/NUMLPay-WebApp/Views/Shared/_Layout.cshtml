<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - NUMLPay</title>

    <!-- bootstrap css -->
    <link rel="stylesheet" type="text/css" href="~/css/bootstrap.home.min.css">
    <!-- style css -->
    <link rel="stylesheet" type="text/css" href="~/css/style.css">
    <!-- Responsive-->
    <link rel="stylesheet" href="~/css/responsive.css">
    <!-- Alerts-->
    <link rel="stylesheet" href="~/css/alerts.css">
    <!-- fevicon -->
    <link rel="icon" href="~/images/numl-logo.png" />
    <!-- Scrollbar Custom CSS -->
    <link rel="stylesheet" href="~/css/jquery.mCustomScrollbar.min.css">
    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
          rel="stylesheet" />

    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <!-- owl stylesheets -->
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")

    <script>
        function openLoginPage() {
            window.location.href = "/Home/Login";
        }

        function openRegisterPage() {
            window.location.href = "/Home/Register";
        }
    </script>

    <style>
        .navbar {
            position: relative;
            z-index: 9999;
        }
    </style>

</head>
<body>

    <!-- nav section start-->
    <nav class="navbar navbar-expand-lg navbar-light bg-light navbar-default navbar-static-top" style="height: 50px;">
        <a class="navbar-brand" href="#">
            <img src="~/images/logo.png" alt="Logo" width="150" height="150">
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" style="background-color: white;" id="navbarNav">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Index#home">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Index#about">About</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Index#service">Services</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Index#team">Team</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Index#contact">Contact</a>
                </li>
            </ul>
        </div>
    </nav>
    <!-- nav section end-->


    <div>
        <hr />

        @RenderBody()

    </div>

    <!--footer section start -->
    <div class="footer_section layout_padding">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-sm-6">
                    <h4 class="about_text">About NUMLPay</h4>
                    <p class="dolor_text">
                        Simplifying Transactions, Empowering NUML Students.
                    </p>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <h4 class="about_text">Contact NUMLPay</h4>
                    <div class="location_text"><img src="~/images/map-icon.png"><span class="padding_left_15">H 9/4 H-9, Islamabad</span></div>
                    <div class="location_text"><img src="~/images/call-icon.png"><span class="padding_left_15">+92 344 7818962</span></div>
                    <div class="location_text"><img src="~/images/mail-icon.png"><span class="padding_left_15">numlpay@gmail.com</span></div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <h4 class="about_text">Useful Links</h4>
                    <div class="location_text"><img width="20" height="20" src="~/images/round-1.png">  <a href="/Home/Index#home" style="color:white;">  Home</a></div>
                    <div class="location_text"><img width="20" height="20" src="~/images/round-1.png">  <a href="/Home/Index#contact" style="color:white;">  Contact Us</a></div>
                    <div class="location_text"><img width="20" height="20" src="~/images/round-1.png">  <a href="/Home/Login" style="color:white;">  Login</a></div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="footer_social_icon">
                        <ul>
                            <li><a href="#"><img src="~/images/fb-icon1.png"></a></li>
                            <li><a href="#"><img src="~/images/twitter-icon1.png"></a></li>
                            <li><a href="#"><img src="~/images/linkedin-icon1.png"></a></li>
                            <li><a href="#"><img src="~/images/youtub-icon1.png"></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- copyright section start -->
            <div class="copyright_section">
                <div class="copyright_text">Copyright 2023 All Right Reserved By <a href="https://hanannawaz.com">Hanannawaz.com</a></div>
            </div>
            <!-- copyright section end -->
        </div>
    </div>
    <!--footer section end -->
    <!-- Javascript files-->
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/jquery-3.0.0.min.js"></script>
    <script src="~/js/plugin.js"></script>
    <!-- sidebar -->
    <script src="~/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- javascript -->
    <script src="~/js/owl.carousel.js"></script>
    <script src="https:cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.js"></script>

</body>
</html>
